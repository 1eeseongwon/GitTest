package pack01_Variable;

public class Ex03_Variable { 
	public static void main(String[] args) {
		
		int value = 0 ; // 변수 선언하기
		int result = value + 10; 
		
//		int value = 10;
//		int result = value + 10;
		// 숫자형 변수들은 사칙연산이 가능하다
		// 재할당 (값을 다시 줘서 바꿈)
		result = 1+1+10;
		System.out.println(result);
		result = 10-9;
		System.out.println(result);
		
		
	}

}
