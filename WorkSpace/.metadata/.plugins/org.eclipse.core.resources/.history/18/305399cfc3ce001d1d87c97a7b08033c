
public class Test06 {
	public static void main(String[] args) {
		int korScore, mathScore, engScroe, sciScore, peScore;
		korScore = 90;
		mathScore = 90;
		engScroe = 90;
		sciScore = 90;
		peScore = 90;
		// 네이밍룰: Class 파일은 대문자로 시작
		// 변수는 소문자로 시작하고, 의미 있는 단어 연결해서 조합
		System.out.println(korScore);
		
	}
}
